	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"postinstall": "rimraf ./.cache/webpack",
		"clean": "node ./preprocess/utils/clean.js",
		"ndb": "ndb .",
		"start": "rimraf ./.dev && nodemon ./server/index.js --config ./preprocess/server/.nodemon --inspect",
		"babel": "babel-node ./server/index.js --config-file ./preprocess/server/babel.start.config.js -r ./node_modules/ndb/inspect --inspect",
		"build": "run-s clean build:server build:next",
		"build:server": "webpack --config ./preprocess/server/webpack.config.js --env.production",
		"build:next": "next build",
		"analyze": "cross-env BUNDLE_ANALYZE=both run-s build",
		"analyze:client": "cross-env BUNDLE_ANALYZE=browser run-s build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server run-s build",
		"serve": "cross-env NODE_ENV=production node ./.next/onp-academy.js",
		"unserve": "pm2 stop all && pm2 delete all"
	},
